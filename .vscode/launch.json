    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Start Server",
            "type": "node-terminal",
            "request": "launch",
            "preLaunchTask": "Start Sql Server",
            "command": "ASPNETCORE_ENVIRONMENT=Development && dotnet watch --project ${workspaceFolder}/SocialWorkApi.csproj --launch-profile https",
            "serverReadyAction": {
                "action": "openExternally",
                "uriFormat": "%s/swagger",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            }
        },
    ]
}